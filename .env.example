# Firebase Configuration
PUBLIC_FIREBASE_API_KEY=your-api-key-here
PUBLIC_FIREBASE_AUTH_DOMAIN=your-auth-domain.firebaseapp.com
PUBLIC_FIREBASE_DATABASE_URL=https://your-database.firebaseio.com
PUBLIC_FIREBASE_PROJECT_ID=your-project-id
PUBLIC_FIREBASE_STORAGE_BUCKET=your-storage-bucket.appspot.com
PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your-sender-id
PUBLIC_FIREBASE_APP_ID=your-app-id

# Environment
PUBLIC_ENV=development

# Features
PUBLIC_ENABLE_ANALYTICS=false
PUBLIC_ENABLE_ERROR_TRACKING=false